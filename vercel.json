{
  "version": 2,
  "buildCommand": "npm --prefix app/frontend run build",
  "outputDirectory": "app/frontend/dist",
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://your-railway-backend-url.railway.app/api/:path*"
    }
  ]
}
