# Test Database Configuration
# For testing, we use mock database mode to avoid Prisma initialization issues
# In production, set up a real test database
DATABASE_URL="postgresql://user:password@localhost:5432/earntrack_test?schema=public"
USE_MOCK_DATABASE="true"

# JWT Configuration
JWT_SECRET="test-secret-key-for-testing-only"
JWT_EXPIRES_IN="1h"

# Server Configuration
PORT=3001
NODE_ENV="test"

# CORS Configuration
ALLOWED_ORIGINS="http://localhost:5173,http://localhost:3000"

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
